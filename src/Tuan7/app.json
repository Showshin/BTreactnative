{
  "plugins": [
    [
      "expo-sqlite",
      {
        "enableFTS": true,
        "useSQLCipher": false,
        "android": {
          "enableFTS": false,
          "useSQLCipher": false
        },
        "ios": {
          "customBuildFlags": [
            "-DSQLITE_ENABLE_DBSTAT_VTAB=1 -DSQLITE_ENABLE_SNAPSHOT=1"
          ]
        }
      }
    ],
    "expo-router"
  ],
  "name": "my-app",
  "slug": "my-app",
  "android": {
    "package": "com.anonymous.myapp"
  }
}
